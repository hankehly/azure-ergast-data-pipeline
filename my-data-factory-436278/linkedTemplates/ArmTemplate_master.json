{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "my-data-factory-436278"
		},
		"datalake78543_azure_blob_storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datalake78543_azure_blob_storage'"
		},
		"datalake78543_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake78543_service'"
		},
		"sampleappdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sampleappdb'"
		},
		"workspace54378_appdb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'workspace54378_appdb_service'"
		},
		"Ergast REST API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ergast.com/api/f1/"
		},
		"datalake78543_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake78543.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "my-data-factory-436278_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake78543_azure_blob_storage_connectionString": {
						"value": "[parameters('datalake78543_azure_blob_storage_connectionString')]"
					},
					"datalake78543_service_accountKey": {
						"value": "[parameters('datalake78543_service_accountKey')]"
					},
					"sampleappdb_connectionString": {
						"value": "[parameters('sampleappdb_connectionString')]"
					},
					"workspace54378_appdb_service_connectionString": {
						"value": "[parameters('workspace54378_appdb_service_connectionString')]"
					},
					"Ergast REST API_properties_typeProperties_url": {
						"value": "[parameters('Ergast REST API_properties_typeProperties_url')]"
					},
					"datalake78543_service_properties_typeProperties_url": {
						"value": "[parameters('datalake78543_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "my-data-factory-436278_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'my-data-factory-436278_ArmTemplate_0')]"
			]
		}
	]
}